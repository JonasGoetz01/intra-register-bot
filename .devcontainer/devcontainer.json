{
    "name": "Python Dev Container",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "mounts": [
		"source=${localEnv:HOME}/.ssh,target=/root/.ssh,type=bind,consistency=cached"
	],
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
    },
    "extensions": [
        "ms-python.python"
    ],
    "remoteUser": "root"
}